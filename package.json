        "prettier:controller:check": "prettier --check --ignore-path .gitignore app/Http/Controllers --tab-width 4 --print-width 80",
        "prettier:controller:fix": "prettier --write --ignore-path .gitignore app/Http/Controllers --tab-width 4 --print-width 80",
        "prettier:model:check": "prettier --check --ignore-path .gitignore app/Models --tab-width 4 --print-width 80",
        "prettier:model:fix": "prettier --write --ignore-path .gitignore app/Models --tab-width 4 --print-width 80",
        "prettier:react": "prettier --write --ignore-path .gitignore resources/js --tab-width 2 --print-width 80"
    },
